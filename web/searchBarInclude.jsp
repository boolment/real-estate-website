<section id="searchBar">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <div class="searchBarBody">
                    <form>
                        <div class="searchWrapper searchLocation">
                            <label>Enter Your Prefered Location</label>
                            <input type="search" placeholder="Enter Your Location ">
                            <span class="icon-location-pin"></span>
                        </div>
                        <div class="searchWrapper searchPropertyType">
                            <label>Property Type</label>
                            <select>
                                <option value="flat">Full Flat</option>
                                <option value="pg">PG Shared</option>
                                <option value="hostel">Hostel</option>
                                <option value="rooms">Rooms/House</option>
                            </select>
                        </div>
                        <div class="searchWrapper searchWho">
                            <label>Who</label>
                            <select>
                                <option value="flat">Single Boys</option>
                                <option value="pg">Single Girls</option>
                                <option value="hostel">Family</option>
                                <option value="rooms">Guest</option>
                            </select>
                        </div>
                        <div class="searchWrapper searchButton">
                            
                            <input type="button" value="search">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>