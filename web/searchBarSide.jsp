<div class="searchBarInclude">
    <h5><i class="icon-eye"> </i> Search Your Property</h5>
    <div class="searchBody">
        <form>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label for="location" class="control-label"><i class="icon-map"></i> Location</label>
                        <input type="text" class="form-control" placeholder="Enter Your Locality" name="" id="location">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12 col-md-6">
                    <div class="form-group">
                        <label class="control-label"><i class="icon-wallet"></i> Min Budget</label>
                        <input type="number" name="" class="form-control" placeholder="15k">
                    </div>
                </div>
                <div class="col-sm-12 col-md-6">
                    <div class="form-group">
                        <label class="control-label">Max Budget</label>
                        <input type="number" name="" class="form-control" placeholder="25k">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label for="searchfor" class="control-label"><i class="icon-people"></i> Accommodation For</label>
                        <select class="form-control" id="searchfor">
                            <option>Girls</option>
                            <option>Boys</option>
                            <option>Family</option>
                            <option>Any</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label for="searchAccommoType" class="control-label"><i class="icon-share"></i> Accommodation Type</label>
                        <select class="form-control" id="searchAccommoType">
                            <option>Shared Room</option>
                            <option>Private Room</option>
                            <option>Entire House</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label for="searchBedRooms" class="control-label"><i class="fa fa-bed"> </i> Number Of Bedrooms</label>
                        <select class="form-control" id="searchBedRooms">
                            <option>1 BHK</option>
                            <option>2 BHK</option>
                            <option>3 BHK</option>
                            <option>ALL BHK</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label for="searchHouseType" class="control-label"><i class="fa fa-building"> </i> House Type</label>
                        <select class="form-control" id="searchHouseType">
                            <option>Flat</option>
                            <option>House</option>
                            <option>PG</option>
                            <option>Rooms</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label for="searchFurnish" class="control-label">Furnishing Type</label>
                        <select class="form-control" id="searchFurnish">
                            <option>Fully Furnished</option>
                            <option>Semi Furnished</option>
                            <option>Unfurnished</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <button class="filter-search">Search Homes</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>